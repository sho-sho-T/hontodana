{
	"name": "hontodana",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"start": "next start",
		"lint": "npx biome lint .",
		"lint:fix": "npx biome lint --write .",
		"format": "npx biome format .",
		"format:fix": "npx biome format --write .",
		"check": "npx biome check .",
		"check:fix": "npx biome check --write .",
		"lint:next": "next lint",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"db:migrate": "npx prisma migrate dev",
		"db:migrate:init": "npx prisma migrate dev --name init",
		"db:migrate:reset": "npx prisma migrate reset",
		"db:migrate:deploy": "npx prisma migrate deploy",
		"db:migrate:status": "npx prisma migrate status",
		"db:generate": "npx prisma generate",
		"db:studio": "npx prisma studio",
		"db:push": "npx prisma db push",
		"db:pull": "npx prisma db pull",
		"db:seed": "tsx prisma/seed.ts",
		"db:reset": "npx prisma migrate reset --force && npm run db:seed",
		"db:setup": "npm run db:migrate:init && npm run db:generate && npm run db:seed"
	},
	"dependencies": {
		"@anthropic-ai/claude-code": "^1.0.43",
		"@next-auth/prisma-adapter": "^1.0.7",
		"@prisma/client": "^6.13.0",
		"@prisma/extension-accelerate": "^2.0.2",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-slot": "^1.2.3",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.52.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"gsap": "^3.13.0",
		"lucide-react": "^0.525.0",
		"next": "15.3.5",
		"next-auth": "^4.24.11",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"tailwind-merge": "^3.3.1",
		"zod": "^4.0.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@tailwindcss/postcss": "^4",
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/react": "^16.3.0",
		"@types/jest": "^30.0.0",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/supertest": "^6.0.3",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"prisma": "^6.13.0",
		"supertest": "^7.1.4",
		"tailwindcss": "^4",
		"ts-jest": "^29.4.1",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5"
	}
}
